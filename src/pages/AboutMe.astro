---
title: "About Me";
import { Button } from "@nextui-org/react";

const skills = [
	{
		link: "data:image/png;base64,",
		name: "HTML",
	},
	{
		link: "data:image/png;base64,",
		name: "TailwindCss",
	},
	{
		link: "data:image/png;base64,",
		name: "JavaScript",
	},
	{
		link: "data:image/png;base64,",
		name: "React.js",
	},
	{
		link: "data:image/png;base64,",
		name: "Figma",
	},
	{
		link: "data:image/png;base64,",
		name: "Node.js",
	},
	{
		link: "data:image/png;base64,",
		name: "Mongo.db",
	},
	{
		link: "data:image/png;base64,",
		name: "Express.js",
	},
	{
		link: "data:image/png;base64,",
		name: "Python",
	},
	{
		link: "data:image/png;base64,",
		name: "Java",
	},
];
---

<main class="max-w-5xl mx-auto py-20 flex">
	<section class="p-5 w-2/5">
		<div class="text-center">
			<h1 class="dark:text-purple-300 text-5xl font-bold mb-4">Marc Puig</h1>
			<div class="">
				<span
					class="bg-gray-300 text-gray-900 text-xs font-medium px-2.5 py-0.5 rounded-full dark:bg-gray-700 dark:text-gray-300"
					>Web designer</span
				>
				<span
					class="bg-gray-300 text-gray-900 text-xs font-medium px-2.5 py-0.5 rounded-full dark:bg-gray-700 dark:text-gray-300"
					>Full Stack dev</span
				>
			</div>
			<Button
				color="secondary"
				variant="ghost"
				className="text-sm p-2 mt-4"
				id="downloadCV">
				Download Resume
